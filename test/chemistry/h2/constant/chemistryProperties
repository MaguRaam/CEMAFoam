FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    solver            ode;
    method            cemaChemistryModel;
}

jacobian exact;

chemistry       on;

initialChemicalTimeStep 1e-7;

EulerImplicitCoeffs
{
    cTauChem        1;
}

nElements 4; // TODO Extract from species info in the future

odeCoeffs
{
    solver          seulex;
    absTol          1e-12;
    relTol          1e-1;
}

